{
  "source": {
    "include": [
      "./utils"
    ],
    "exclude": [
      "./utils/route-utils.js",
      "./utils/event-decorator.jsx",
      "./utils/promise-ajax-decorator.jsx"
    ],
    "includePattern": ".+\\.js([doc|x])?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md",
    "template": "node_modules/docdash"
  },
  "docdash": {
    "sort": false
  }
}